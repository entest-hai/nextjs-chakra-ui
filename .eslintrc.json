{
  "root": true,
  "extends": "next",
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "indent": [
      "error",
      2
    ],
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": [
          "^_",
          "colorMode"
        ]
      }
    ],
    "react/display-name": 0
  }
}